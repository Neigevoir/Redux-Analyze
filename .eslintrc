{
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "extends": "react-app",
  "globals": {
    "React": true,
    "_": true,
    "wx": true
  },
  "plugins": ["jsx-a11y", "prettier", "react"],
  "rules": {
    "prettier/prettier": "error",
    "jsx-a11y/href-no-hash": [0],
    "jsx-a11y/anchor-has-content": [0],
    "jsx-a11y/img-has-alt": [0]
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.7"
    }
  }
}
